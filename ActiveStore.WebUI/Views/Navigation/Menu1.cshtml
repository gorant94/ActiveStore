@model IEnumerable<string>
  
<link href="~/Content/Menu1.css" rel="stylesheet" />
<style>
    #menu1
    {
        margin-bottom: 20px;
    }

</style>

<ul id="menu1" >
    <a  href="/" >ActiveStore.ua - Ваш путь в мир приключений</a>
    <li>@Html.ActionLink("Главная", "Main", "Main")</li>
    <li>
        <a href="#">Категории</a>
        <ul>
            @foreach (var link in Model)
            {
                <li>
                    
                        @Html.RouteLink(link, new
           {
               controller = "Product",
               action = "List",
               category = link,
               page = 1
           })
                    
                </li>
            }
        </ul>
    </li>
    <li>@Html.ActionLink("Покупка", "Index", "Cart")</li>
    <li>@Html.ActionLink("Доставка", "Index", "Cart")</li>
    <li>@Html.ActionLink("Контакты", "Contacts", "Contacts")</li>
    <li>@Html.ActionLink("Гарантия", "Contacts", "Contacts")</li>
    
 
    @Html.Action("Summary", "Cart")

   
</ul>









